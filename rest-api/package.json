{
  "name": "hungry-web-app-restapi",
  "version": "1.0.0",
  "description": "RESTApi for handling hungry web app requests",
  "main": "server.js",
  "scripts": {
    "start": "node .",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
    "prod": "tsc && node ./www/server.js",
    "clean": "rm -rf www/ || true",
    "build": "npm run clean && tsc && cp -rf src/config www/config && cp .npmrc www/.npmrc && cp package.json www/package.json && cd www && zip -r Archive.zip . && cd ..",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markmavromatis/hungry-web-app.git"
  },
  "author": "Mark Mavromatis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markmavromatis/hungry-web-app/issues"
  },
  "homepage": "https://github.com/markmavromatis/hungry-web-app#readme",
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.0.13",
    "@types/sequelize": "^4.28.9",
    "@types/validator": "^13.0.0",
    "bcrypt": "^5.0.0",
    "chai": "^4.2.0",
    "email-validator": "^2.0.4",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^7.2.0",
    "pg": "^8.2.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.12",
    "sequelize-typescript": "^1.1.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.2",
    "typescript": "^3.9.5"
  }
}
